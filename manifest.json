{
	"name": "SigToCal",
	"version": "1.0",
	"description": "Sigarra To Calendar - Automatically add Sigarra Timetables to your Calendar Apps",
	"permissions": [
		"https://*.up.pt/*"
	],
	"browser_action": {
		"default_title": "SigToCal",
		"default_icon": "icons/icon.png"
	},
	"content_scripts": [{
		"matches": ["https://*.up.pt/*hor_geral*", "https://*.up.pt/*HOR_GERAL*"],
		"css": ["css/add_btn.css"],
		"js": ["scripts/jquery.min.js", "scripts/utils.js", "scripts/ics.deps.min.js", "scripts/ics.min.js", "extractors/base.js", "extractors/timetable.js", "main.js"],
		"run_at": "document_end"
	}],
	"manifest_version": 2,
	"content_security_policy": "script-src 'self'; object-src 'self'"
}